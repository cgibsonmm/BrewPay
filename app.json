{
  "expo": {
    "name": "BrewPay",
    "slug": "BrewPay",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mainIcon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "splash": {
        "image": "./assets/img/splash.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "org.markup.exp"
    },
    "android": {
      "splash": {
        "mdpi": "./assets/img/splash-mdpi.png",
        "hdpi": "./assets/img/splash-hdpi.png",
        "xhdpi": "./assets/img/splash-xhdpi.png",
        "xxhdpi": "./assets/img/splash-xxhdpi.png",
        "xxxhdpi": "./assets/img/splash-xxxhdpi.png",
        "resizeMode": "cover"
      },
      "package": "org.markup.exp"
    }
  }
}
